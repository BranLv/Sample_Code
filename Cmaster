public class Cmaster{


public static void main(String[] args){
 //create frame
     JFrame mainWindow = new JFrame("Code Masters");
     mainWindow.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
     mainWindow.setPreferredSize(new Dimension(500,500));


 //create panel
     JPanel panel1 = new JPanel();
     panel1.setBackground(Color.BLUE);

 //create buttons
     JButton DisplayButton = new JButton("DISPLAY");
    
     
  //create label
     JLabel print = new JLabel();
     print.setText("Image displayed: " );
     
 //Image intitialized
 Buffered image1;
 
//actionlistener for Displaybutton






 //add to primary label
      panel1.add(print);
      panel1.add(DisplayButton);
    
      
      //pack everything
     mainWindow.getContentPane().add(panel1);
     mainWindow.pack();
     
     
     //Make Visibe
     mainWindow.setVisible(true);
}
